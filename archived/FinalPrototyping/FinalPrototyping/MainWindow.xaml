<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalPrototyping"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        x:Class="FinalPrototyping.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel Margin="10">
        <Button DockPanel.Dock="Bottom" Width="75" Height="25" HorizontalAlignment="Right" Margin="0 10 0 0" Click="Button_Click">Start2</Button>
        <Button DockPanel.Dock="Bottom" Width="75" Height="25" HorizontalAlignment="Right" Margin="0 10 0 0" Click="Button_Click">Start1</Button>
        <ProgressBar DockPanel.Dock="Bottom" Name="progressBar" Height="25" Margin="0 10 0 0"></ProgressBar>
        <TextBlock DockPanel.Dock="Bottom" Name="textBlock" Margin="0 10 0 0">Status</TextBlock>
        <Border DockPanel.Dock="Top" BorderBrush="Black" BorderThickness="1">
            <Wpf:WebView2 Name="webView" />
        </Border>
    </DockPanel>
</Window>
