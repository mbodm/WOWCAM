<Window x:Class="WOWCAMUPD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WOWCAMUPD"
        mc:Ignorable="d"
        Title="WOWCAMUPD" Width="640" Height="400" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <DockPanel Margin="10">
        <Label DockPanel.Dock="Top" Padding="0 0 0 3">Log:</Label>
        <TextBox DockPanel.Dock="Bottom" IsReadOnly="True" FontFamily="Courier New" Name="textBoxLog" />
    </DockPanel>
</Window>
